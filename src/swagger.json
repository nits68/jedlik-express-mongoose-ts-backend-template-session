                        "description": "Ennyi rekordot hagyunk el (skip) a lekérdezés 'elejéről'.",
                        "in": "path",
                        "schema": {
                            "type": "number",
                            "example": "0"
                        },
                        "required": true
                    },
                    {
                        "name": "limit",
                        "description": "Ennyi rekordot küld (take) maximum az API.",
                        "in": "path",
                        "schema": {
                            "type": "number",
                            "example": "2"
                        },
                        "required": true
                    },
                    {
                        "name": "field",
                        "description": "Melyik mező szerint legyenek rendezve a rekordok. A kötőjel '-' karakter a csökkenő rendezést jelöli.",
                        "in": "path",
                        "schema": {
                            "type": "string",
                            "example": "-cim"
                        },
                        "required": true
                    },
                    {
                        "name": "filter",
                        "description": "Szűrő - a 'cim' és a 'leiras' mezőkben keressük.",
                        "in": "path",
                        "schema": {
                            "type": "string",
                            "example": "sport"
                        },
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "count": {
                                            "type": "number",
                                            "description": "A szűrőnek megfelelő rekordok száma",
                                            "example": "16"
                                        },
                                        "data": {
                                            "type": "array",
                                            "description": "A maximum {limit} számú, szűrőnek megfelelő rekord.",
                                            "items": {
                                                "$ref": "#/components/schemas/hirdetes"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "error.message"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "schemas": {
            "recipe": {
                "type": "object",
                "required": [
                    "nev"
                ],
                "properties": {
                    "_id": {
                        "type": "string",
                        "description": "PK - A recept azonosítója (ObjectID).",
                        "example": "61daf48ba5775848e99f555b"
                    },
                    "nev": {
                        "type": "string",
                        "maxLength": 30,
                        "uniqueItems": true,
                        "description": "A kategória neve.",
                        "example": "Munkagép"
                    }
                }
            },
            "kategoria_populate": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/recipe"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "kategoria_hirdetesei": {
                                "type": "array",
                                "items": {
                                    "$ref": "#/components/schemas/hirdetes"
                                }
                            }
                        }
                    }
                ]
            },
            "hirdetes": {
                "type": "object",
                "required": [
                    "cim",
                    "kategoria_id",
                    "vetelar"
                ],
                "properties": {
                    "_id": {
                        "type": "number",
                        "description": "PK - A hirdetés azonosítója.",
                        "example": 2
                    },
                    "kategoria_id": {
                        "type": "number",
                        "description": "FK - A kategória azonosítója.",
                        "example": 3
                    },
                    "cim": {
                        "type": "string",
                        "uniqueItems": true,
                        "maxLength": 100,
                        "description": "A hirdetés címe.",
                        "example": "BELARUS MTZ 82"
                    },
                    "evjarat": {
                        "type": "string",
                        "maxLength": 10,
                        "description": "A jármű évjárata pl: 1995 vagy 2019/08",
                        "example": "2022/11"
                    },
                    "km_allas": {
                        "type": "number",
                        "description": "A járművel eddig megtett távolság [km]",
                        "example": 210000
                    },
                    "szin": {
                        "type": "string",
                        "description": "A jármű szine",
                        "example": "ezüst"
                    },
                    "uzemanyag": {
                        "type": "string",
                        "description": "A jármű által használt üzemanyag típusa.",
                        "example": "Benzin",
                        "enum": ["Benzin", "Dízel", "Elektromos", "Hibrid", "Gáz"]
                    },
                    "hengerurtartalom": {
                        "type": "number",
                        "description": "A jármű lökettérfogata, ha értelmezhető, -1 értékű, ha nem. [cm2]",
                        "example": 1890
                    },
                    "teljesitmeny": {
                        "type": "number",
                        "description": "A jármű teljesítménye [LE]",
                        "example": 1890
                    },
                    "serulesmentes": {
                        "type": "boolean",
                        "description": "Igaz értékű, ha az autó nem volt sérült.",
                        "example": true
                    },
                    "leiras": {
                        "type": "string",
                        "description": "A hirdetés szövege.",
                        "example": "MTZ 82 traktor, eredeti piros. ..."
                    },
                    "hirdetes_datum": {
                        "type": "string",
                        "description": "A hirdetés feladásának dátuma.",
                        "example": "2023-01-02"
                    },
                    "vetelar": {
                        "type": "number",
                        "description": "A jármű meghirdetett vételára [Forint].",
                        "example": 2500000
                    },
                    "kepek": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "maxLength": 300
                        },
                        "description": "A hirdetett jármű fényképeinek URL címe.",
                        "example": "[https://nitslaszlo.github.io/static/hahu/68a.jpg, https://nitslaszlo.github.io/static/hahu/68b.jpg]"
                    }
                }
            },
            "hirdetes_populate": {
                "allOf": [
                    {
                        "$ref": "#/components/schemas/hirdetes"
                    },
                    {
                        "type": "object",
                        "properties": {
                            "hirdetes_kategoria": {
                                "type": "object",
                                "$ref": "#/components/schemas/recipe"
                            }
                        }
                    }
                ]
            }
        }
    }
}